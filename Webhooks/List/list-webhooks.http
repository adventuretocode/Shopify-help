POST https://{{$dotenv SHOPIFY_DOMAIN}}.myshopify.com/admin/api/{{$dotenv SHOPIFY_VERSION}}/graphql.json
Content-Type: application/json
X-Shopify-Access-Token: {{$dotenv SHOPIFY_TOKEN}}
X-REQUEST-TYPE: GraphQL

query listWebhooks {
  webhookSubscriptions(first: 20) {
    edges {
      node {
        id
        topic
				callbackUrl
				endpoint {
          __typename
        }
      }
    }
  }
}
